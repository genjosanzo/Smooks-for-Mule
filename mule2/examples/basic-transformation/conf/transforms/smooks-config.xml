<?xml version="1.0"?>
<smooks-resource-list xmlns="http://www.milyn.org/xsd/smooks-1.0.xsd">

	<!--
		On the "b" element, when it is a child of the "c" element,
		a XSLT transformation is done using the defined xsl file.
	 -->
	<resource-config selector="c b">
	    <resource>/transforms/BasicXslTransform.xsl</resource>
    </resource-config>

</smooks-resource-list>
